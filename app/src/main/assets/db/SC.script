CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE PUBLIC.RECIPE(RECIPENAME VARCHAR(40) NOT NULL PRIMARY KEY, CATEGORY VARCHAR(15), COOKINGLEVEL VARCHAR(10), PREPTIME INTEGER, COOKINGTIME INTEGER, SERVING INTEGER, INSTRUCTIONS VARCHAR(1500))
CREATE MEMORY TABLE PUBLIC.INGREDIENTS(RECIPENAME VARCHAR(40) NOT NULL, INGREDIENT VARCHAR(50), CONSTRAINT CONSTRAINT1 PRIMARY KEY(RECIPENAME,INGREDIENT), CONSTRAINT CONSTRAINT2 FOREIGN KEY(RECIPENAME) REFERENCES RECIPE(RECIPENAME))
CREATE MEMORY TABLE PUBLIC.KEYINGREDIENTS(RECIPENAME VARCHAR(40) NOT NULL, KEYINGREDIENT VARCHAR(50), CONSTRAINT CONSTRAINT3 PRIMARY KEY(RECIPENAME,KEYINGREDIENT), CONSTRAINT CONSTRAINT4 FOREIGN KEY(RECIPENAME) REFERENCES RECIPE(RECIPENAME))
CREATE MEMORY TABLE PUBLIC.COMMENTS(RECIPENAME VARCHAR(40) NOT NULL, USERNAME VARCHAR(40) NOT NULL, COMMENT VARCHAR(250), RATING INTEGER, CONSTRAINT CONSTRAINT5 PRIMARY KEY(RECIPENAME,USERNAME),CONSTRAINT CONSTRAINT6 FOREIGN KEY(RECIPENAME) REFERENCES RECIPE(RECIPENAME))
